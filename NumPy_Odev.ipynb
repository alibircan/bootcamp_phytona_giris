   "source": [
    "def kenar(rowN,colN):\n",
    "    k=np.zeros((rowN,colN))\n",
    "    k[0,:]=1\n",
    "    k[rowN-1,:]=1\n",
    "    k[:,0]=1\n",
    "    k[:,colN-1]=1\n",
    "    return k\n",
    "kenar(5,7)"
   ]
